{{#section 'head'}}
    <!-- Meta Tags -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,700&display=swap" rel="stylesheet">
    <!-- CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet"/>
    <link rel="stylesheet" href="../../public/css/box.css">
    <link rel="stylesheet" href="../../public/css/explore.css">
    <link rel="stylesheet" href="../../public/css/cards.css">
    <!-- Scripts -->
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <script src="../../scripts/landing.js"></script>
    <title>Nook</title>
{{/section}}

<!-- Org Selector -->
<header class="mb-2 mt-3">
    <div class="container mt-2">
        <div id="type-selector">
            <a href="#" value="CSO" id = "cso">cso</a>
            <a href="#" value="NON-CSO" id = "non-cso" >non-cso</a>
            <a href="#" value= "OTHERS" id = "others">others</a>
        </div>
    </div>
</header>
<!-- Org Dashboard -->
<section class="pl-5 pr-5 pt-4">
    <div class="container" class="card-wrapper" style="max-width: 960px;">
        <div class="card-menu" id="explore-menu">
            <div class="row">
                
                {{#fromDB}}
                    {{#if Orgs}}
                        {{#each Orgs}}
                            <div class="card">
                                <a href="vieworg">
                                    <img class="card-img-top img-fluid" src="{{this.org_logo}}">
                                    <div class="card-body">
                                        <span>{{this.org_name}}</span>
                                    </div>
                                </a>
                            </div>
                        {{/each}}
                    {{else}}
                        <p>No orgs yet</p>
                    {{/if}}

                {{/fromDB}}

            </div>
        </div>
    </div>
</section>