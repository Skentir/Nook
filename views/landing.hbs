{{#section 'head'}}
    <!-- CSS -->
    <link rel="stylesheet" href="../../public/css/styling.css">
    <script src="../../scripts/landing.js"></script> 
    <!-- Google Sign-In -->
    <meta name="google-signin-client_id" content="473203977419-114n43ik4g8rcjvfech0ckgddv0250c3.apps.googleusercontent.com">
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    <title>Nook</title>
{{/section}}

<!-- Landing Page Section -->
<section class="container custom-container mt-1">
    <div class="box">
        <div class="inner-box">
            <div class="right-box">
                <header>
                    <h1>nook</h1>
                </header>
                <div class="description">
                    <span>
                        A centralized space to
                        explore student organizations and events in
                        DLSU Manila.
                    </span>
                </div>
                <div>
                    <div class="row ml-0">
                        <a href="#" id="get-started" class="custombtn" data-toggle="modal" data-target="#get-started-modal">
                            Get Started 
                        </a>
                    </div>
                </div>
            </div>
            <figure class="left-box">
                <img src="../../public/assets/browsing.svg" id="landing-pic" alt="Nook welcome girl">
            </figure>
        </div>
    </div>
</section>
<!-- Sign in Modal -->
<section class="modal fade" role="dialog" id="sign-in-modal">
    <div class="modal-dialog modal-dialog-centered">
        <div class="box modal-content">
            <div class="inner-box">
                <button type="button" class="close" data-dismiss="modal">
                    &times;
                </button>
                <figure class="modal-right">
                    <img src="../../public/assets/modern-art.svg" class="modal-pic" alt="Nook register girl">
                </figure>
                <div class="modal-right">
                    <header>
                        <h5 class="modal-title" style="font-size: 30px;">nook</h5>
                    </header>
                    <!-- Log in Form -->
                    <div class="modal-body">
                        <form name ="log_in" method='POST' action='/login'>                       
                            <div class="form-group input-group mb-2">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="inputGroup-sizing-sm" style="background: #D0CDE1;">
                                        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                                            <rect width="20" height="20" fill="url(#pattern0)"/>
                                            <defs>
                                            <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                            <use xlink:href="#image0" transform="scale(0.00195312)"/>
                                            </pattern>
                                            <image id="image0" width="512" height="512" xlink:href="data:image/png;base64,"/>
                                            </defs>
                                        </svg> 
                                    </span>
                                </div>
                                <input type="text" class="form-control" id="user-id" placeholder=" Email" name ="email">
                            </div>

                            <div class="form-group input-group mb-2">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="inputGroup-sizing-sm" style="background: #D0CDE1;">
                                        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
